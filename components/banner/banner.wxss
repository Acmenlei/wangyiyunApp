/* components/banner.wxss */
.container {
  width: 100%;
  text-align: center;
}
.container image {
  border-radius: 10rpx;
}
.container .swiper-item {
  position: relative;
}
.container .swiper-item label {
  position: absolute;
  right: 55rpx;
  bottom: 50rpx;
  font-size: 20rpx;
  padding: 10rpx 20rpx;
  color: white;
  opacity: .8;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}